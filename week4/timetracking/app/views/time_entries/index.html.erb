<h1> <%= @my_project.name %> </h1>

<p> time entries </p>
<ul>

	<% @my_entries.each do |entry|%>
	<li>
		<%= entry.hours %> h
		<%= entry.minutes %> m
		<%= entry.date %>

		<%= link_to 'edit', edit_project_time_entry_path(@my_project,entry)%>
	</li>
	
	<%end%>

	<%= link_to "add new time entry", new_project_time_entry_path(@my_project)%>

</ul>