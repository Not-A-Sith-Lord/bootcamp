<h3> add a concert </h3>

<%=form_for @my_concert do |f|%>

	<%= f.label :artist %>
	<%= f.text_field :artist%>

	<%= f.label :venue %>
	<%= f.text_field :venue%>

	<%= f.label :city %>
	<%= f.text_field :city%>

	<%= f.label :date %>
	<%= f.date_field :date%>

	<%= f.label :price %>
	<%= f.text_field :price%>

	<%= f.label :description %>
	<%= f.text_field :description%>


	<%= f.button%>


<% end %>
