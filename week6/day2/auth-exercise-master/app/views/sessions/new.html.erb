<h1> login </h1>

<p class="need-to-login">
<%= flash[:need_to_login_message] %>
</p>


<%= form_tag "/login" do %>

	<div>
	<%= label_tag :email %>
	<%= email_field_tag :email %>
	</div>

	<div>
	<%= label_tag :password %>
	<%= password_field_tag :password %>
	</div>

	<%= button_tag "Login"%>

<% end %>